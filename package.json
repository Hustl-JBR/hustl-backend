{
  "name": "hustl-backend",
  "version": "1.0.0",
  "description": "Hustl backend with Express, Neon Postgres, Stripe, and more",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node scripts/migrate-and-start.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "cleanup:test-users": "node scripts/cleanup-test-users.js --list",
    "cleanup:test-users:delete": "node scripts/cleanup-test-users.js --delete"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "stripe": "^19.3.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "resend": "^3.2.0",
    "zod": "^3.22.4",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.16.0",
    "prisma": "^5.9.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "tsx": "^4.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17"
  }
}
